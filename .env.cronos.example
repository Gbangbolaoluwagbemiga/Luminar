# Cronos Network Configuration for Luminar
# Copy this file to .env and fill in your values

# Private Key (DO NOT COMMIT THIS FILE WITH REAL VALUES)
PRIVATE_KEY=your_private_key_here

# Cronos RPC URLs
CRONOS_RPC_URL=https://evm.cronos.org
CRONOS_TESTNET_RPC_URL=https://evm-t3.cronos.org

# Cronos Explorer API Key (for contract verification)
# Get your API key from https://cronoscan.com/myapikey
CRONOSCAN_API_KEY=your_cronoscan_api_key_here

# x402 Configuration
X402_FACILITATOR_URL=https://x402-facilitator.crypto.com
X402_API_KEY=your_x402_api_key_here

# Crypto.com AI Agent SDK
CRYPTO_COM_AI_SDK_KEY=your_ai_sdk_key_here

# Frontend Environment Variables (use NEXT_PUBLIC_ prefix)
NEXT_PUBLIC_LUMINAR_CONTRACT=0x... # Will be filled after deployment
NEXT_PUBLIC_CRONOS_RPC_URL=https://evm.cronos.org
NEXT_PUBLIC_CRONOS_CHAIN_ID=25
NEXT_PUBLIC_CRONOS_TESTNET_CHAIN_ID=338
NEXT_PUBLIC_WCRO_ADDRESS=0x5C7F8A570d578ED84E63fdFA7b1eE72dEae1AE23
NEXT_PUBLIC_USDC_ADDRESS=0xc21223249CA28397B4B6541dfFaEcC539BfF0c59
NEXT_PUBLIC_USDT_ADDRESS=0x66e428c3f67a68878562e79A0234c1F83c208770
NEXT_PUBLIC_X402_ENABLED=true
